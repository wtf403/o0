{
  "name": "browser-launcher",
  "version": "1.0.0",
  "description": "MacOS browser launcher with floating URL bar",
  "main": "src/index.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder .",
    "rebuild": "electron-rebuild .",
    "website": "cd website && live-server",
    "website:build": "postcss website/css/*.css --dir build/css",
    "website:watch": "postcss website/css/*.css --dir build/css --watch"
  },
  "keywords": [],
  "author": "wtf403",
  "license": "MIT",
  "build": {
    "appId": "com.browser.launcher",
    "mac": {
      "category": "public.app-category.utilities"
    }
  },
  "devDependencies": {
    "autoprefixer": "^10.4.17",
    "electron": "^34.0.0",
    "electron-builder": "^25.1.8",
    "electron-rebuild": "^3.2.9",
    "live-server": "^1.2.2",
    "node-gyp": "^10.0.1",
    "nodemon": "^2.0.22",
    "postcss": "^8.4.35",
    "postcss-cli": "^11.0.0",
    "postcss-preset-env": "^9.3.0"
  },
  "dependencies": {
    "active-win": "^8.1.1",
    "bindings": "^1.5.0",
    "nan": "^2.18.0",
    "node-addon-api": "^8.3.0"
  },
  "gypfile": true,
  "browserslist": [
    "defaults and fully supports es6-module",
    "maintained node versions"
  ]
}
